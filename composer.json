{
    "name": "edderrd/exchanges-bccr",
    "description": "Central Bank Dolar Exchanges Parser",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Edder Rojas",
            "email": "edder.rojas@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "fabpot/goutte": "^3.2",
        "nesbot/carbon": "^1.29",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "ExchangesBCCR\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExchangesBCCR\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ExchangesBCCR\\ExchangesBCCRServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
